<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.5//EN" "http://struts.apache.org/dtds/struts-2.5.dtd">

<struts>
    <package name="interceptor" namespace="/jsp/interceptor" extends="struts-default">
    
		<interceptors>
         	<interceptor name = "myinterceptor" class = "interceptor.MyInterceptor" />
      	</interceptors>
    
        <action name="execute" class="interceptor.InterAction" method="execute">
        	<interceptor-ref name = "myinterceptor" />
		    <result name="success">/jsp/interceptor/result.jsp</result>
		</action>
        
    </package>
</struts>